         <image>
            <Image url="@../../../images/help.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="30" fitWidth="30" layoutX="324.0" layoutY="197.0">
         <clip>
            <Circle centerX="15" centerY="15" fill="aqua" radius="15" />
         </clip>
         <image>
            <Image url="@../../../images/volume.png" />
         </image>
      </ImageView>
      <Button layoutX="146.0" layoutY="118.0" mnemonicParsing="false" text="ranking list" />
      <Button layoutX="146.0" layoutY="154.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="76.0" text="Continue" />
      <Button fx:id="game_button" layoutX="146.0" layoutY="78.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="76.0" onAction="#gameButtonClick" text="play" />
